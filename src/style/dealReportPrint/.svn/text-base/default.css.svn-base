@CHARSET "UTF-8";
body{margin:0;padding:0;font-size:12px;font-family:"宋体"; overflow-x:hidden;}
html,ul,li,dl,dt,dd,ol,th,td,form,h1,h2,h3,h4,h5,h6,img,p{margin:0;padding:0;border:none;}
img{vertical-align:middle;}
button,select,textarea{outline:none}
textarea{resize:none}
ul,li{list-style-type:none;}
.clear{clear:both;}
a{color:#008bbf;text-decoration:none;outline:none;}
a:hover{text-decoration:underline;}
/*去掉a标签超链接访问过后出现的虚线框*/
a:visited,a:active{outline:none;star:expression(this.onFocus=this.blur());}
body a{outline:none;blr:expression(this.onFocus=this.blur());}
i,font,em{font-style:normal;}
b,strong,h4{font-weight:normal;}
/*我的Call客级别*/
.Teltitle{ position:relative; background:#50b4e6; height:45px; line-height:45px; font-size:28px; color:#FFF; font-family:"微软雅黑"; padding:0 0 0 11px;border-radius: 4px;cursor:pointer;}
